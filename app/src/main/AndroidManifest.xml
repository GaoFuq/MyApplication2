<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.gfq.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- 适配屏幕宽高比为18:9的手机 -->
        <meta-data
            android:name="android.max_aspect"
            android:value="2.1"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.TeamDialogActivity"
            android:theme="@style/MyDialogActivityTheme"
            />
        <activity android:name=".activity.BagDialogActivity"
            android:theme="@style/MyDialogActivityTheme"
            />
        <activity android:name=".activity.BagActivity"
            android:theme="@style/MyDialogActivityTheme"
            />
    </application>

</manifest>